{
    "name": "mkastoun/laravel-419-handler",
    "description": "Graceful handling of Laravel 419 session expired errors.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Laravel419Handler\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Laravel419Handler\\Laravel419HandlerServiceProvider"
            ]
        }
    },
    "require": {
        "php": ">=8.0",
        "illuminate/support": "^9.0|^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0|^8.0|^9.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Laravel419Handler\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
